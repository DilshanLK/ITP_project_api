{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"], 
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-use-before-define":"off",
    "no-await-in-loop":"off",
    "no-plusplus":"off",
    "no-console":"off",
    "consistent-return":"off",
    "camelcase":"off"
  }
}
